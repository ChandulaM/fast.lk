<?xml version="1.0" encoding="UTF-8"?>
<api context="/payment" name="PaymentService" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="GET">
        <inSequence>
            <log>
                <property name="Payment" value="Payment Service Called"/>
            </log>
            <send>
                <endpoint key="Payment"/>
            </send>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
